<div class="container">
  <div class="row">
    <div class="col-md-6 seperator">
      {{create-order blends=model.blends createOrder='createOrder'}}
    </div>
    <div class="col-md-6 seperator2">
      {{#each model.orders as |order|}}
        {{#unless order.fulfilled}}
          {{order-list-tile order=order}}
          <hr>
        {{/unless}}  
      {{/each}}
    </div>
  </div>
</div>
